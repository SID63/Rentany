{
  "name": "rent-any",
  "version": "0.1.0",
  "description": "A modern rental platform built with Next.js, connecting renters with property owners seamlessly",
  "private": true,
  "author": "Rent Any Team",
  "keywords": [
    "rental",
    "property",
    "nextjs",
    "typescript",
    "tailwindcss",
    "vercel"
  ],
  "homepage": "https://rentany.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rentany/rent-any.git"
  },
  "bugs": {
    "url": "https://github.com/rentany/rent-any/issues"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "postbuild": "next-sitemap",
    "vercel-build": "npm run build",
    "preview": "npm run build && npm run start"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
